{
  "name": "basic-record",
  "version": "1.0.0",
  "description": "A 25 LOC utility to define and create records via objects literals.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "npm run cjs && npm run fix && npm test",
    "cjs": "ascjs esm cjs",
    "fix": "sed -i 's/(m => .* m)//' cjs/index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc node test.js"
  },
  "keywords": [
    "record",
    "struct",
    "mixins",
    "basic",
    "simple"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^3.1.2",
    "coveralls": "^3.0.9",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "@ungap/get-own-property-descriptors": "^1.0.0"
  }
}
